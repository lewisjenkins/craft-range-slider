{
    "name": "lewisjenkins/craft-range-slider",
    "description": "An easy-to-use, flexible and responsive range slider for Craft CMS.",
    "type": "craft-plugin",
    "version": "4.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft range slider",
        "lj range slider"
    ],
    "support": {
        "docs": "https://github.com/lewisjenkins/craft-range-slider/blob/master/README.md",
        "issues": "https://github.com/lewisjenkins/craft-range-slider/issues",
        "email": "lewis@lj.io"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Lewis Jenkins",
            "homepage": "https://lj.io"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "lewisjenkins\\craftrangeslider\\": "src/"
        }
    },
    "extra": {
        "name": "LJ Range Slider",
        "handle": "craft-range-slider",
        "developer": "Lewis Jenkins",
        "developerUrl": "https://lj.io",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/lewisjenkins/craft-range-slider/master/CHANGELOG.md",
        "class": "lewisjenkins\\craftrangeslider\\CraftRangeSlider"
    }
}
